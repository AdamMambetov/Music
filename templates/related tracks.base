formulas:
  FileName: file.asLink(aliases[0])
views:
  - type: table
    name: Table
    filters:
      and:
        - file.path.contains("Tracks")
        - or:
            - this.related.contains(file)
            - file.properties.related.contains(this.file)
        - this.file.ext != "base"
    order:
      - formula.FileName
      - aliases
      - SourceFile
